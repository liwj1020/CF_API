.*-- List presentation area:
:LIST     DEPTH='*'
          LISTDEF=DTLLST
          MAXHEAD=2
          VIEW=ALTVIEW
          ACTOR=UIM
          SCROLL=YES
          PARMS=CMDPRM.
 
:TOPINST .Type options, press Enter.
 
:LISTACT  ENTER='CMD CHGWSE SBSD(&SBSLIB/&SBSNAM) WRKSTNTYPE(&WKSTYP)'
          ENTER='JOBD(&JBDNMQ) MAXACT(&MAXAJB) AT(&CTLJOB)'
          ENTER='MAXACT(&MAXAJB)'
          PROMPT='CMD ?CHGWSE ?*SBSD(&SBSLIB/&SBSNAM) ?-WRKSTN()'
          PROMPT='?*WRKSTNTYPE(&WKSTYP) ??JOBD(&JBDNMQ)'
          PROMPT='??MAXACT(&MAXAJB) ??AT(&CTLJOB)'
          HELP='WRKWSE/OPTCHG'
          OPTION=2
          NOCMD=PROMPT
          .2=Change
 
:LISTACT  ENTER='CMD ADDWSE SBSD(&SBSLIB/&SBSNAM) WRKSTNTYPE(&WKSTYP)'
          ENTER='JOBD(&JBDNMQ) MAXACT(&MAXAJB) AT(&CTLJOB)'
          PROMPT='CMD ?ADDWSE ?*SBSD(&SBSLIB/&SBSNAM) ?-WRKSTN()'
          PROMPT='??WRKSTNTYPE(&WKSTYP) ??JOBD(&JBDNMQ)'
          PROMPT='??MAXACT(&MAXAJB) ??AT(&CTLJOB)'
          HELP='WRKWSE/OPTCPY'
          OPTION=3
          NOCMD=PROMPT
          .3=Copy
 
:LISTACT  ENTER='CMD RMVWSE SBSD(&SBSLIB/&SBSNAM) WRKSTNTYPE(&WKSTYP)'
          PROMPT='CMD ?RMVWSE ?*SBSD(&SBSLIB/&SBSNAM) ?-WRKSTN()'
          PROMPT='?*WRKSTNTYPE(&WKSTYP)'
          HELP='WRKWSE/OPTRMV'
          OPTION=4
          CONFIRM=CNFPNL
          USREXIT='CALL EXITPG'
          .4=Remove
 
:LISTACT  ENTER='CMD WRKJOBD JOBD(&JBDNMQ)'
          PROMPT='CMD ?WRKJOBD ?*JOBD(&JBDNMQ)'
          HELP='WRKWSE/OPTWJD'
          OPTION=5
          .5=Work with Job description
 
:LISTCOL  VAR=OPTION
