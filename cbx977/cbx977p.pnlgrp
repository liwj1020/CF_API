          PROMPT='?*OPTION(*SPLF) &CMDPRM'
          HELP='WRKJOBS/OPTSPL'
          OPTION=8
          .8=Spooled files
 
:LISTACT  ENTER='CMD DSPJOBLOG JOB(&JOBNBR/&JOBUSR/&JOBNAM) '
          ENTER='&CMDPRM'
          PROMPT='CMD ?DSPJOBLOG ?*JOB(&JOBNBR/&JOBUSR/&JOBNAM) '
          PROMPT='&CMDPRM'
          HELP='WRKJOBS/OPTJBL'
          OPTION=10
